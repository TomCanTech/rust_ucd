    "version": "0.2.0",
    "configurations": [
        
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in library 'rust_ucd'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--lib",
                    "--package=rust_ucd"
                ],
                "filter": {
                    "name": "rust_ucd",
                    "kind": "lib"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'rust_ucd'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=rust_ucd",
                    "--package=rust_ucd",
                ],
                "filter": {
                    "name": "rust_ucd",
                    "kind": "bin"
                }
            },
            "args": ["--db-path", "C:\\Users\\Shepherd\\Documents\\Personal Projects\\Programming\\rust\\rust_ucd\\target\\debug\\rust_ucd.db"],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in executable 'rust_ucd'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bin=rust_ucd",
                    "--package=rust_ucd"
                ],
                "filter": {
                    "name": "rust_ucd",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        }
    ]
}